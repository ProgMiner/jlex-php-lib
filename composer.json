{
    "name": "progminer/php-lex",
    "description": "JLexPHP PHP port",
    "authors": [
        {
            "name": "Eridan Domoratskiy",
            "email": "eridan200@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "nikic/php-parser": "^4.0.3",
        "progminer/php-datagen-lib": "master-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "progminer/php-datagen": "master-dev"
    },
    "autoload": {
        "psr-4": {
            "PHPLex\\": "src"
        },
        "classmap": [
            "pdg-classes"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
